{
    "plugins": [
        "react"
    ],
    "extends": ["plugin:react/recommended"],
    "settings": {
        "react": {
            "version": "16.0"
        }
    },
    "parser": "babel-eslint",
    "env": {
        "node": false,
        "browser": true,
        "commonjs": true,
        "es6": true
    },
    "globals": {
        "__webpack_public_path__": true
    }
}
